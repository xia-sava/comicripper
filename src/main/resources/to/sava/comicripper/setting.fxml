<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<BorderPane xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="to.sava.comicripper.controller.SettingController"
            fx:id="settingScene"
            stylesheets="@common.css"
>
    <center>
        <VBox alignment="TOP_CENTER">
            <GridPane fx:id="settingGrid" hgap="10.0" vgap="10.0">
                <padding>
                    <Insets top="5.0" right="5.0" bottom="5.0" left="5.0"/>
                </padding>
            </GridPane>
            <Button fx:id="close" text="閉じる"/>
        </VBox>
    </center>
</BorderPane>
